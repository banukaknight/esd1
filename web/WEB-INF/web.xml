<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>com.Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>com.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApproveMemberships</servlet-name>
        <servlet-class>com.ApproveMemberships</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakePayment</servlet-name>
        <servlet-class>com.MakePayment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewMembers</servlet-name>
        <servlet-class>com.ViewMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewClaims</servlet-name>
        <servlet-class>com.ViewClaims</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterMember</servlet-name>
        <servlet-class>com.RegisterMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewOutstandingBalances</servlet-name>
        <servlet-class>com.ViewOutstandingBalances</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewProvisionalMembers</servlet-name>
        <servlet-class>com.ViewProvisionalMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewUserClaimsAndPayments</servlet-name>
        <servlet-class>com.ViewUserClaimsAndPayments</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/MainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApproveMemberships</servlet-name>
        <url-pattern>/ApproveMemberships</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakePayment</servlet-name>
        <url-pattern>/MakePayment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewMembers</servlet-name>
        <url-pattern>/ViewMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewClaims</servlet-name>
        <url-pattern>/ViewClaims</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterMember</servlet-name>
        <url-pattern>/RegisterMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewOutstandingBalances</servlet-name>
        <url-pattern>/ViewOutstandingBalances</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewProvisionalMembers</servlet-name>
        <url-pattern>/ViewProvisionalMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewUserClaimsAndPayments</servlet-name>
        <url-pattern>/ViewClaimsAndPayments</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            20
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MainController</welcome-file>
    </welcome-file-list>
</web-app>
